@{
}
<ul>
    @foreach (string milestone in ViewBag.events)
    {
        <li>
            @milestone
        </li>
    }
</ul>
<p>
    <a asp-controller="Events" asp-action="Add">Add Event</a>
</p>
@if(ViewBag.events.Count == 0)
{
    <p>No events yet!</p>
}