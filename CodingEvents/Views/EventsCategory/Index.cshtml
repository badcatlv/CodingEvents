@{
}
@using CodingEvents.Models
@model List<EventCategory>

<h1>Event Category</h1>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Category Name</th>
    </tr>
    @foreach(EventCategory category in Model)
    {
        <tr>
            <td>@category.Id</td>
            <td>@category.Name</td>
        </tr>
    }
</table>

